// Java program for single inheritence
class Parent
{
   public void Print()
   {
      System.out.println(" It is a parent class");
   }
}
class child extent Parent
{
   public void Print1()
   {
      System.out.println(" It is a child class");
   }
}
public class Main 
{
    public static void main(String[] args)
    {
       child c = new child();
       c.Print();
       c.Print1();
     }
 }
