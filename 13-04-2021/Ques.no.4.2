// Java program for multilevel inheritence
class Parent
{
   public void Print()
   {
      System.out.println(" It is a parent class");
   }
}
class child1 extent Parent
{
   public void Print1()
   {
      System.out.println(" It is a child1 class");
   }
}
class child2 extent child1
{
   public void Print2()
   {
      System.out.println(" It is a child2 class");
   }
}
public class Main 
{
    public static void main(String[] args)
    {
       child2 c = new child2();
       c.Print();
       c.Print1();
       c.Print2();
     }
 }
